{
  "name": "My Extension",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Gets information from the page.",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Make this page red",
    "default_icon": {
      "16": "images/ecos16.png",
      "32": "images/ecos32.png",
      "48": "images/ecos48.png",
      "120": "images/ecos120.png"
    }
  },
  "icons": {
    "16": "images/ecos16.png",
    "32": "images/ecos32.png",
    "48": "images/ecos48.png",
    "120": "images/ecos120.png"
  },
  "permissions": ["tabs", "activeTab"]
}